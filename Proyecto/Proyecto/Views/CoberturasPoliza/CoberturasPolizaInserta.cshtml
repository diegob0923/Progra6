@model Proyecto.Models.sp_Retorna_Cobertura_De_Poliza_Result

@{

    ViewBag.Title = "CoberturasPolizaInserta";
    Layout = "~/Views/Shared/_LayoutSolo.cshtml";
}


<h2>Insertar coberturas póliza</h2>

<form id="frmCoberturasPolizaInserta" method="post">

    <div class="form-group">
        <label>Nombre :</label>
        <input type="text" name="Nombre" id="Nombre" class="form-control" />
    </div>
    <div class="row">
        <label class="col-sm-2"></label>
        <div class="col-sm-10">
            <p id="Estatus" />
        </div>
    </div>
    <div class="form-group">
        <label>Descripción</label>
        <input type="text" name="Descripcion" id="Descripcion" class="form-control" />
    </div>
    <div class="form-group">
        <label>Porcentaje :</label>
        <input type="text" name="Porcentaje" id="Porcentaje" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Registrar" id="btnRegistrar" />
    </div>
    <a href="~/CoberturasPoliza/CoberturasPolizaLista">Regresar a la lista</a>

</form>

@section scripts{

    @Scripts.Render("~/Scripts/ScriptsProyecto/CoberturasPolizaInserta.js");
}