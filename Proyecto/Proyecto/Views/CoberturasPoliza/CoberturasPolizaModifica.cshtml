@model Proyecto.Models.sp_Retorna_Cobertura_De_PolizaID_Result
@{
    ViewBag.Title = "CoberturasPolizaModifica";
    Layout = "~/Views/Shared/_LayoutSolo.cshtml";
}

<h2 align="center">Modificar cobertura de póliza</h2>

<form align="center" id="frmCoberturasPolizaModifica" method="post">
    <div align="center" class="form-group">
        <label>Nombre :</label>
        <input type="text" name="Nombre" id="Nombre" class="form-control" value="@Model.Nombre" />
    </div>
    <div align="center" class="form-group">
        <label>Descripción</label>
        <input type="text" name="Descripcion" id="Descripcion" class="form-control" value="@Model.Descripcion" />
    </div>
    <div align="center" class="form-group">
        <label>Porcentaje :</label>
        <input type="text" name="Porcentaje" id="Porcentaje" class="form-control" value="@Model.Porcentaje" />
    </div>
    <div align="center" class="form-group">
        <input class="btn btn-primary btn-block mb-4" type="submit" value="Modificar" id="btnModificar" />
    </div>
    <a href="~/CoberturasPoliza/CoberturasPolizaLista">Regresar a la lista</a>
</form>
@section scripts{
    @Scripts.Render("~/Scripts/ScriptsProyecto/CoberturasPolizaModifica.js");

}