@model Proyecto.Models.sp_Retorna_AdiccionesID_Result
@{
    ViewBag.Title = "AdiccionesModificar";
    Layout = "~/Views/Shared/_LayoutSolo.cshtml";
}


<h2 align="center">Modificar adicción</h2>

<form align="center" id="frmAdicciones" method="post">
    <div align="center" class="form-group">
        <label>Nombre</label>
        <input type="text" name="Nombre" id="Nombre" class="form-control" value="@Model.Nombre" />
    </div>
    <div align="center" class="form-group">
        <label>Código</label>
        <input type="text" name="Codigo" id="Codigo" class="form-control" value="@Model.Codigo" />
    </div>
    <div align="center" class="form-group">
        <input class="btn btn-primary btn-block mb-4" type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/Adicciones/AdiccionesLista">Regresar a la lista</a>
</form>
@section scripts{
    <script src="~/Scripts/ScriptsProyecto/AdiccionesValidacion.js"></script>
}