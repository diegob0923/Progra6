@{
    ViewBag.Title = "AdiccionesInsertar";
    Layout = "~/Views/Shared/_LayoutSolo.cshtml";
}




<h2 align="center">Insertar adicciones</h2>
<form align="center" id="frmAdicciones" method="post">
    <div align="center" class="form-group">
        <label>Nombre</label>
        <input type="text" name="Nombre" id="Nombre" class="form-control" />
    </div>
    <div align="center" class="form-group">
        <label>Código</label>
        <input type="text" name="Codigo" id="Codigo" class="form-control" />
    </div>
    <div align="center" class="form-group">
        <input class="btn btn-primary btn-block mb-4" type="submit" value="Registrar" id="btnRegistrar" />
    </div>
    <a href="~/Adicciones/AdiccionesLista">Regresar a la lista</a>
</form>
@section scripts{
    <script src="~/Scripts/ScriptsProyecto/AdiccionesValidacion.js"></script>
}